[PrefixDeclaration]
:		http://www.semanticweb.org/ontologies/MaterializeTest.owl#

[MappingDeclaration] @collection [[
mappingId	MAPID-T1
target		:id/{id} a :B . 
source		SELECT id FROM T1

mappingId	MAPID-T2
target		:id/{id} :p "{p1}" .
source		SELECT id,  p1 FROM T2

mappingId	MAPID-T3
target		:id/{id} :p "{p1}" .
source		SELECT id, p1 FROM T3

mappingId	MAPID-T4
target		:id/{id} :p "{p1}" .
source		SELECT id, p1 FROM T4

mappingId	MAPID-T5
target		:id/{id} :p "{p1}" .
source		SELECT id, p1 FROM T5

mappingId	MAPID-T6
target		:id/{id} :p "{p1}" .
source		SELECT * FROM T6

mappingId	MAPID-T7
target		:id/{id} :p {p1}@en . 
source		SELECT * FROM T7

mappingId	MAPID-T8-es
target		:id/{id} :p {p1}@es .
source		SELECT * FROM T8 WHERE p2 = 'es'

mappingId	MAPID-T8-en
target		:id/{id} :p {p1}@en .
source		SELECT * FROM T8 WHERE p2 = 'en'

mappingId	MAPID-T8-fr
target		:id/{id} :p {p1}@fr .
source		SELECT * FROM T8 WHERE p2 = 'fr'

mappingId	MAPID-T9
target		:id/{id} :q :id/{id2} . 
source		SELECT * FROM T9
]]

