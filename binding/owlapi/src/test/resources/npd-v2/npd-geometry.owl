  </rdf:Description>
  <rdf:Description rdf:about="http://sws.ifi.uio.no/vocab/npd-v2#SeismicSurveyCoordinate">
    <rdfs:subClassOf rdf:resource="http://sws.ifi.uio.no/vocab/npd-v2#Point"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://sws.ifi.uio.no/vocab/npd-v2#SurveyArea">
    <rdfs:subClassOf rdf:resource="http://sws.ifi.uio.no/vocab/npd-v2#Area"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://sws.ifi.uio.no/vocab/npd-v2#SurveyMultilineArea">
    <rdfs:subClassOf rdf:resource="http://sws.ifi.uio.no/vocab/npd-v2#Area"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://sws.ifi.uio.no/vocab/npd-v2#WellboreCoordinate">
    <rdfs:subClassOf rdf:resource="http://sws.ifi.uio.no/vocab/npd-v2#Point"/>
  </rdf:Description>
  <owl:DatatypeProperty rdf:about="http://sws.ifi.uio.no/vocab/npd-v2#currentAreaSize"/>
  <owl:AnnotationProperty rdf:about="http://sws.ifi.uio.no/vocab/npd-v2#definition"/>
  <owl:ObjectProperty rdf:about="http://sws.ifi.uio.no/vocab/npd-v2#isGeometryOfFeature">
    <rdfs:comment>Note: There is no geos:hasFeature. This property is created for reasons of convenience.</rdfs:comment>
    <rdfs:isDefinedBy>http://sws.ifi.uio.no/vocab/version/20130919/npd-geometry</rdfs:isDefinedBy>
    <owl:inverseOf rdf:resource="http://www.opengis.net/ont/geosparql#hasGeometry"/>
  </owl:ObjectProperty>
  <owl:Ontology rdf:about="http://sws.ifi.uio.no/vocab/version/20130919/npd-geometry">
    <dc:source rdf:resource="http://www.npd.no/en/About-us/Information-services/Dictionary/"/>
    <owl:imports rdf:resource="http://sws.ifi.uio.no/vocab/version/20130919/npd-db"/>
    <owl:imports rdf:resource="http://www.opengis.net/ont/geosparql"/>
  </owl:Ontology>
  <owl:Class rdf:about="http://www.opengis.net/ont/geosparql#SpatialObject"/>
  <owl:ObjectProperty rdf:about="http://www.opengis.net/ont/geosparql#hasGeometry"/>
</rdf:RDF>
