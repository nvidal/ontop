    <rdfs:subClassOf rdf:resource="http://sws.ifi.uio.no/vocab/npd-v2#Licensee"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://sws.ifi.uio.no/vocab/npd-v2#FieldOperator">
