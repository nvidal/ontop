npdv:AwardArea
    sql:columns "apaAreaNetGeometryWKT,apaAreaType,blkId,blkLabel,blkName,prvName,qdrName,urlNPD (table: apaAreaNet)" ;
    sql:label "APA (Awards in predefined areas) net areas. (table: apaAreaNet)" ;
    sql:table "apaAreaNet" ;
    a owl:Class ;
    rdfs:subClassOf [
        sql:construction "apaAreaNet.blkLabel IS NOT NULL" ;
        a owl:Restriction ;
        owl:onProperty npdv:blockLocation ;
        owl:someValuesFrom owl:Thing
    ], [
        sql:construction "apaAreaNet.apaAreaNetGeometryWKT IS NOT NULL" ;
        a owl:Restriction ;
        owl:onProperty geos:asWKT ;
        owl:someValuesFrom rdfs:Literal
    ], [
        sql:construction "apaAreaNet.blkId IS NOT NULL" ;
        a owl:Restriction ;
        owl:onProperty npdv:blockId ;
        owl:someValuesFrom rdfs:Literal
    ], [
        sql:construction "apaAreaNet.qdrName IS NOT NULL" ;
        a owl:Restriction ;
        owl:onProperty npdv:quadrantLocation ;
        owl:someValuesFrom owl:Thing
    ], [
        sql:construction "apaAreaNet.urlNPD IS NOT NULL" ;
        a owl:Restriction ;
        owl:onProperty npdv:npdPageURL ;
        owl:someValuesFrom owl:Thing
    ], [
        sql:construction "apaAreaNet.prvName IS NOT NULL" ;
        a owl:Restriction ;
        owl:onProperty npdv:provinceLocation ;
        owl:someValuesFrom owl:Thing
    ] .

