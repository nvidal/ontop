npdv:DSTBottomHolePressure
    sql:column "wellbore_dst.wlbDstBottomHolePress" ;
    sql:comment "The downhole pressure opposite a formation of interest when the valves at the surface (or downhole) are completely closed. The pressure is transmitted by fluids which exist in a formation and are in communication with the wellbore. (column: wlbDstBottomHolePress)" ;
    sql:datatype "DECIMAL(13,6) (column: wlbDstBottomHolePress)" ;
    sql:label "Bottom hole pressure [MPa] (column: wlbDstBottomHolePress)" ;
    a owl:DatatypeProperty ;
    rdfs:domain npdv:WellboreDrillStemTest ;
    rdfs:range xsd:decimal .

npdv:DSTChokeSize
    sql:column "wellbore_dst.wlbDstChokeSize" ;
    sql:comment "The choke is a type of orifice installed in a well flow line for the purpose of restricting the flow and controlling the rate of production. During testing of a well, the flow will often be measured through a series of different choke diameters. (column: wlbDstChokeSize)" ;
    sql:datatype "DECIMAL(13,6) (column: wlbDstChokeSize)" ;
    sql:label "Choke size [mm] (column: wlbDstChokeSize)" ;
    a owl:DatatypeProperty ;
    rdfs:domain npdv:WellboreDrillStemTest ;
    rdfs:range xsd:decimal .

npdv:DSTDepthFrom
    sql:column "wellbore_dst.wlbDstFromDepth" ;
    sql:comment "Top depth of the zone that is opened up for flow, measured depth. (column: wlbDstFromDepth)" ;
    sql:datatype "DECIMAL(13,6) (column: wlbDstFromDepth)" ;
    sql:label "From depth MD [m] (column: wlbDstFromDepth)" ;
    a owl:DatatypeProperty ;
    rdfs:domain npdv:WellboreDrillStemTest ;
    rdfs:range xsd:decimal .

npdv:DSTDepthTo
    sql:column "wellbore_dst.wlbDstToDepth" ;
    sql:comment "Bottom depth of the zone that is opened up for flow, measured depth. (column: wlbDstToDepth)" ;
    sql:datatype "DECIMAL(13,6) (column: wlbDstToDepth)" ;
    sql:label "To depth MD [m] (column: wlbDstToDepth)" ;
    a owl:DatatypeProperty ;
    rdfs:domain npdv:WellboreDrillStemTest ;
    rdfs:range xsd:decimal .

npdv:DSTFinalFlowPressure
    sql:column "wellbore_dst.wlbDstFinFlowPress" ;
    sql:comment "The pressure determined at the formation face just prior to shut-in for a build-up test. (column: wlbDstFinFlowPress)" ;
    sql:datatype "DECIMAL(13,6) (column: wlbDstFinFlowPress)" ;
    sql:label "Final flow pressure [MPa] (column: wlbDstFinFlowPress)" ;
    a owl:DatatypeProperty ;
    rdfs:domain npdv:WellboreDrillStemTest ;
    rdfs:range xsd:decimal .

npdv:DSTFinalShutInPressure
    sql:column "wellbore_dst.wlbDstFinShutInPress" ;
    sql:comment "Pressure as recorded at the wellhead when the valve is closed and the well is shut-in in the final build-up period. (column: wlbDstFinShutInPress)" ;
    sql:datatype "DECIMAL(13,6) (column: wlbDstFinShutInPress)" ;
    sql:label "Final shut-in pressure [MPa] (column: wlbDstFinShutInPress)" ;
    a owl:DatatypeProperty ;
    rdfs:domain npdv:WellboreDrillStemTest ;
    rdfs:range xsd:decimal .

