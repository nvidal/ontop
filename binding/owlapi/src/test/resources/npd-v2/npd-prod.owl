<?xml version="1.0" encoding="utf-8"?>
<rdf:RDF
   xmlns:dc="http://purl.org/dc/elements/1.1/"
   xmlns:npd="http://sws.ifi.uio.no/data/npd-v2/"
   xmlns:npdv="http://sws.ifi.uio.no/vocab/npd-v2#"
   xmlns:owl="http://www.w3.org/2002/07/owl#"
   xmlns:ptl="http://sws.ifi.uio.no/vocab/npd-v2-ptl#"
   xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
   xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
   xmlns:xml="http://www.w3.org/XML/1998/namespace"
   xmlns:xsd="http://www.w3.org/2001/XMLSchema#">
  <owl:Class rdf:about="http://sws.ifi.uio.no/vocab/npd-v2#AssociatedGas">
    <npdv:definition>Natural gas dissolved in oil.  Source: http://www.npd.no/en/About-us/Information-services/Dictionary/</npdv:definition>
    <rdfs:isDefinedBy>http://sws.ifi.uio.no/vocab/version/20130919/npd-prod</rdfs:isDefinedBy>
    <rdfs:subClassOf rdf:resource="http://sws.ifi.uio.no/vocab/npd-v2#Gas"/>
  </owl:Class>
  <owl:Class rdf:about="http://sws.ifi.uio.no/vocab/npd-v2#CommercialDiscovery">
    <rdfs:isDefinedBy>http://sws.ifi.uio.no/vocab/version/20130919/npd-prod</rdfs:isDefinedBy>
    <rdfs:subClassOf rdf:resource="http://sws.ifi.uio.no/vocab/npd-v2#Discovery"/>
  </owl:Class>
  <rdf:Description rdf:about="http://sws.ifi.uio.no/vocab/npd-v2#CompanyReserve">
    <rdfs:subClassOf rdf:resource="http://sws.ifi.uio.no/vocab/npd-v2#Reserve"/>
  </rdf:Description>
  <owl:Class rdf:about="http://sws.ifi.uio.no/vocab/npd-v2#CompressedNaturalGas">
    <npdv:definition>CNG (Compressed Natural Gas): Natural gas under pressure in tanks.   Source: http://www.npd.no/en/About-us/Information-services/Dictionary/</npdv:definition>
    <rdfs:isDefinedBy>http://sws.ifi.uio.no/vocab/version/20130919/npd-prod</rdfs:isDefinedBy>
    <rdfs:subClassOf rdf:resource="http://sws.ifi.uio.no/vocab/npd-v2#Gas"/>
  </owl:Class>
  <owl:Class rdf:about="http://sws.ifi.uio.no/vocab/npd-v2#Condensate">
    <npdv:definition>A mixture of the heaviest components of natural gas. Condensate is fluid at normal pressure and temperature.  Source: http://www.npd.no/en/About-us/Information-services/Dictionary/</npdv:definition>
    <rdfs:isDefinedBy>http://sws.ifi.uio.no/vocab/version/20130919/npd-prod</rdfs:isDefinedBy>
    <rdfs:subClassOf rdf:resource="http://sws.ifi.uio.no/vocab/npd-v2#Gas"/>
  </owl:Class>
  <owl:Class rdf:about="http://sws.ifi.uio.no/vocab/npd-v2#ContingentResources">
    <npdv:definition>Recoverable petroleum volumes that have been discovered, but for which no decision has been taken, or permission given, to recover.  Source: http://www.npd.no/en/About-us/Information-services/Dictionary/</npdv:definition>
    <rdfs:isDefinedBy>http://sws.ifi.uio.no/vocab/version/20130919/npd-prod</rdfs:isDefinedBy>
  </owl:Class>
  <owl:Class rdf:about="http://sws.ifi.uio.no/vocab/npd-v2#CrudeOil">
    <npdv:definition>Liquid petroleum from the reservoir. Most of the water and dissolved natural gas have been removed.  Source: http://www.npd.no/en/About-us/Information-services/Dictionary/</npdv:definition>
    <rdfs:isDefinedBy>http://sws.ifi.uio.no/vocab/version/20130919/npd-prod</rdfs:isDefinedBy>
    <rdfs:subClassOf rdf:resource="http://sws.ifi.uio.no/vocab/npd-v2#Oil"/>
  </owl:Class>
  <owl:Class rdf:about="http://sws.ifi.uio.no/vocab/npd-v2#Discovery">
    <npdv:definition>A petroleum deposit, or several petroleum deposits combined, discovered in the same well, and which testing, sampling or logging have shown probably contain mobile petroleum. The definition covers both commercial and technical discoveries. The discovery receives the status of a field, or becomes part of an existing field when a Plan for Development and Operation (PDO) is approved by the authorities (see Field).  Source: http://www.npd.no/en/About-us/Information-services/Dictionary/</npdv:definition>
    <rdfs:isDefinedBy>http://sws.ifi.uio.no/vocab/version/20130919/npd-prod</rdfs:isDefinedBy>
  </owl:Class>
  <rdf:Description rdf:about="http://sws.ifi.uio.no/vocab/npd-v2#DiscoveryReserve">
    <rdfs:subClassOf rdf:resource="http://sws.ifi.uio.no/vocab/npd-v2#Reserve"/>
  </rdf:Description>
  <owl:Class rdf:about="http://sws.ifi.uio.no/vocab/npd-v2#DryGas">
    <npdv:definition>Almost pure methane gas, lacking water and with few heavy components.  Source: http://www.npd.no/en/About-us/Information-services/Dictionary/</npdv:definition>
    <rdfs:isDefinedBy>http://sws.ifi.uio.no/vocab/version/20130919/npd-prod</rdfs:isDefinedBy>
    <rdfs:subClassOf rdf:resource="http://sws.ifi.uio.no/vocab/npd-v2#Gas"/>
  </owl:Class>
  <owl:Class rdf:about="http://sws.ifi.uio.no/vocab/npd-v2#Field">
    <npdv:definition>One discovery, or a number of concentrated discoveries, which the licensees have decided to develop and for which the authorities have approved, or granted exemption for, a Plan for Development and Operation (PDO).  Source: http://www.npd.no/en/About-us/Information-services/Dictionary/</npdv:definition>
    <rdfs:isDefinedBy>http://sws.ifi.uio.no/vocab/version/20130919/npd-prod</rdfs:isDefinedBy>
  </owl:Class>
  <rdf:Description rdf:about="http://sws.ifi.uio.no/vocab/npd-v2#FieldReserve">
    <rdfs:subClassOf rdf:resource="http://sws.ifi.uio.no/vocab/npd-v2#Reserve"/>
  </rdf:Description>
  <owl:Class rdf:about="http://sws.ifi.uio.no/vocab/npd-v2#Gas">
    <rdfs:isDefinedBy>http://sws.ifi.uio.no/vocab/version/20130919/npd-prod</rdfs:isDefinedBy>
    <rdfs:subClassOf rdf:resource="http://sws.ifi.uio.no/vocab/npd-v2#Petroleum"/>
  </owl:Class>
  <owl:Class rdf:about="http://sws.ifi.uio.no/vocab/npd-v2#LiquefiedNaturalGas">
    <npdv:definition>Mainly methane (CH4) transformed into liquid form by cooling.  Source: http://www.npd.no/en/About-us/Information-services/Dictionary/</npdv:definition>
    <rdfs:isDefinedBy>http://sws.ifi.uio.no/vocab/version/20130919/npd-prod</rdfs:isDefinedBy>
    <rdfs:subClassOf rdf:resource="http://sws.ifi.uio.no/vocab/npd-v2#Gas"/>
  </owl:Class>
  <owl:Class rdf:about="http://sws.ifi.uio.no/vocab/npd-v2#LiquefiedPetroleumGas">
    <npdv:definition>Mainly propane (C3H8) and butane (C4H10) transformed into liquid by raising the pressure or cooling.  Source: http://www.npd.no/en/About-us/Information-services/Dictionary/</npdv:definition>
    <rdfs:isDefinedBy>http://sws.ifi.uio.no/vocab/version/20130919/npd-prod</rdfs:isDefinedBy>
    <rdfs:subClassOf rdf:resource="http://sws.ifi.uio.no/vocab/npd-v2#Gas"/>
  </owl:Class>
  <owl:Class rdf:about="http://sws.ifi.uio.no/vocab/npd-v2#NaturalGasLiquid">
    <npdv:definition>A collective term for the petroleum qualities, ethane, propane, isobutane, normal butane and naphtha. NGL are partially liquid at normal pressure.  Source: http://www.npd.no/en/About-us/Information-services/Dictionary/</npdv:definition>
    <rdfs:isDefinedBy>http://sws.ifi.uio.no/vocab/version/20130919/npd-prod</rdfs:isDefinedBy>
    <rdfs:subClassOf rdf:resource="http://sws.ifi.uio.no/vocab/npd-v2#Petroleum"/>
  </owl:Class>
  <owl:Class rdf:about="http://sws.ifi.uio.no/vocab/npd-v2#Oil">
    <npdv:definition>Collective term for crude oil and other liquid petroleum products.  Source: http://www.npd.no/en/About-us/Information-services/Dictionary/</npdv:definition>
    <rdfs:isDefinedBy>http://sws.ifi.uio.no/vocab/version/20130919/npd-prod</rdfs:isDefinedBy>
    <rdfs:subClassOf rdf:resource="http://sws.ifi.uio.no/vocab/npd-v2#Petroleum"/>
  </owl:Class>
  <owl:Class rdf:about="http://sws.ifi.uio.no/vocab/npd-v2#OilEquivalents">
    <npdv:definition>Used when oil, gas, condensate and NGL are to be totalled. The term is either linked to the amount of energy liberated by combustion of the various types of petroleum or to the sales values, so that everything can be compared with oil.   Source: http://www.npd.no/en/About-us/Information-services/Dictionary/</npdv:definition>
    <rdfs:isDefinedBy>http://sws.ifi.uio.no/vocab/version/20130919/npd-prod</rdfs:isDefinedBy>
  </owl:Class>
  <owl:Class rdf:about="http://sws.ifi.uio.no/vocab/npd-v2#Petroleum">
    <npdv:definition>Collective term for hydrocarbons. The term covers all liquid and gaseous hydrocarbons found in a natural state in the substrate, and also other substances recovered in connection with such hydrocarbons.   Source: http://www.npd.no/en/About-us/Information-services/Dictionary/</npdv:definition>
    <rdfs:isDefinedBy>http://sws.ifi.uio.no/vocab/version/20130919/npd-prod</rdfs:isDefinedBy>
  </owl:Class>
  <owl:Class rdf:about="http://sws.ifi.uio.no/vocab/npd-v2#Recovery">
    <npdv:definition>The production of petroleum, including the drilling of production wells, injection, assisted recovery, treatment and storage of petroleum for transport, and loading of petroleum for transportation by ship, as well as the construction, location, operation and use of installations used for recovery.   Source: http://www.npd.no/en/About-us/Information-services/Dictionary/</npdv:definition>
    <rdfs:isDefinedBy>http://sws.ifi.uio.no/vocab/version/20130919/npd-prod</rdfs:isDefinedBy>
  </owl:Class>
  <owl:Class rdf:about="http://sws.ifi.uio.no/vocab/npd-v2#Refining">
    <npdv:definition>The refining of crude oil is really a distillation process. The components with different boiling points are separated in a distillation tower. When heated, the oil is converted to gas, which condenses again at different temperatures to, among others, petrol, paraffin, diesel, heating oils, coke and sulphur.  Source: http://www.npd.no/en/About-us/Information-services/Dictionary/</npdv:definition>
    <rdfs:isDefinedBy>http://sws.ifi.uio.no/vocab/version/20130919/npd-prod</rdfs:isDefinedBy>
  </owl:Class>
  <owl:Class rdf:about="http://sws.ifi.uio.no/vocab/npd-v2#Reserve">
    <npdv:definition>Remaining, recoverable, saleable volumes of petroleum which the licensees have decided to recover and the authorities have given permission to recover.  Source: http://www.npd.no/en/About-us/Information-services/Dictionary/</npdv:definition>
    <rdfs:isDefinedBy>http://sws.ifi.uio.no/vocab/version/20130919/npd-prod</rdfs:isDefinedBy>
  </owl:Class>
  <owl:Class rdf:about="http://sws.ifi.uio.no/vocab/npd-v2#RichGas">
    <npdv:definition>A mixture of wet and dry gas (methane, ethane, propane, butanes, etc.).  Source: http://www.npd.no/en/About-us/Information-services/Dictionary/</npdv:definition>
    <rdfs:isDefinedBy>http://sws.ifi.uio.no/vocab/version/20130919/npd-prod</rdfs:isDefinedBy>
    <rdfs:subClassOf rdf:resource="http://sws.ifi.uio.no/vocab/npd-v2#Gas"/>
  </owl:Class>
  <owl:Class rdf:about="http://sws.ifi.uio.no/vocab/npd-v2#TechnicalDiscovery">
    <rdfs:isDefinedBy>http://sws.ifi.uio.no/vocab/version/20130919/npd-prod</rdfs:isDefinedBy>
    <rdfs:subClassOf rdf:resource="http://sws.ifi.uio.no/vocab/npd-v2#Discovery"/>
  </owl:Class>
  <owl:Class rdf:about="http://sws.ifi.uio.no/vocab/npd-v2#WetGas">
    <npdv:definition>A mixture of gas mainly in liquid phase.   Source: http://www.npd.no/en/About-us/Information-services/Dictionary/</npdv:definition>
    <rdfs:isDefinedBy>http://sws.ifi.uio.no/vocab/version/20130919/npd-prod</rdfs:isDefinedBy>
    <rdfs:subClassOf rdf:resource="http://sws.ifi.uio.no/vocab/npd-v2#Gas"/>
  </owl:Class>
  <owl:AnnotationProperty rdf:about="http://sws.ifi.uio.no/vocab/npd-v2#definition"/>
  <owl:ObjectProperty rdf:about="http://sws.ifi.uio.no/vocab/npd-v2#productionForDiscovery"/>
  <owl:ObjectProperty rdf:about="http://sws.ifi.uio.no/vocab/npd-v2#productionForField"/>
  <owl:Ontology rdf:about="http://sws.ifi.uio.no/vocab/version/20130919/npd-prod">
    <dc:source rdf:resource="http://www.npd.no/en/About-us/Information-services/Dictionary/"/>
    <owl:imports rdf:resource="http://sws.ifi.uio.no/vocab/version/20130919/npd-db"/>
  </owl:Ontology>
</rdf:RDF>
